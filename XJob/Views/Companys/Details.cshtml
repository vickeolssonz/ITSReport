@model XJob.Models.Report
@{
	/**/

	/**/

	ViewBag.Title = "Details";
	Layout = "~/Views/Shared/_Layout.cshtml";
}



<table class="table table-bordered table-hover">
	<tr>
		<th>Title</th>
		<td>@Model.Title</td>
	</tr>
	<tr>
		<th>Description</th>
		<td> @Model.Description</td>
	</tr>
	<tr>
		<th>Username</th>
		<td> @Model.Username</td>
	</tr>
	<tr>
		<th>Phonenumber</th>
		<td> @Model.PhoneNumber</td>
	</tr>
</table>

@using (Html.BeginForm("reportAnalyz", "Companys"))
{
	@Html.HiddenFor(r => r.Id)


<div class="checkbox">
  @Html.CheckBoxFor(m => m.IsDone) Is Done
</div>


<div class="checkbox">
	@Html.CheckBoxFor(m => m.OnGoing) On Going 
</div>

<button type="submit" class="btn btn-primary">Save</button>
}



